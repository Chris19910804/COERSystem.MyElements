{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "coer-elements/components":   ["projects/coer-elements/components/public-api.ts"  ],
      "coer-elements/pages":        ["projects/coer-elements/pages/public-api.ts"       ],
      "coer-elements/directives":   ["projects/coer-elements/directives/public-api.ts"  ],
      "coer-elements/extensions":   ["projects/coer-elements/extensions/public-api.ts"  ],
      "coer-elements/guards":       ["projects/coer-elements/guards/public-api.ts"      ],
      "coer-elements/interceptors": ["projects/coer-elements/interceptors/public-api.ts"],
      "coer-elements/interfaces":   ["projects/coer-elements/interfaces/public-api.ts"  ],
      "coer-elements/pipes":        ["projects/coer-elements/pipes/public-api.ts"       ],
      "coer-elements/signals":      ["projects/coer-elements/signals/public-api.ts"     ],
      "coer-elements/tools":        ["projects/coer-elements/tools/public-api.ts"       ], 
      "coer-elements/services":     ["projects/coer-elements/services/public-api.ts"    ], 
      "@Config/Interfaces":         ["projects/coer-elements-documentation/src/config/app.interfaces"],
      "@Config/Services":           ["projects/coer-elements-documentation/src/config/app.services"  ], 
      "@Config/Environment":        ["projects/coer-elements-documentation/src/config/environments"  ], 
      "@Components":                ["projects/coer-elements-documentation/src/app/shared/components"], 
      "@Interfaces":                ["projects/coer-elements-documentation/src/app/shared/interfaces"], 
      "@Services":                  ["projects/coer-elements-documentation/src/app/shared/services"  ], 
    },
    "outDir": "./dist/out-tsc", 
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "baseUrl": "./"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}